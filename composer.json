{
    "name": "artur/mychatgpt",
    "version": "1.0.0",
    "description": "MyChatGPT",
    "keywords": [
      "MyChatGPT"
    ],
    "homepage": "https://github.com/arturhayne",
    "license": "MIT",
    "authors": [
      {
        "name": "Artur Elias Hayne Oliveira",
        "homepage": "https://github.com/arturhayne"
      }
    ],
    "require": {
      "php": "^8.1",
      "ext-json": "*",
        "openai-php/client": "^0.8.0",
        "symfony/http-client": "^6.3",
        "nyholm/psr7": "^1.8"
    },
    "require-dev": {
      "phpunit/phpunit": "10.0.0"
    },
    "autoload": {
      "psr-4": {
        "MyChatGPT\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
